{
  "expo": {
    "name": "oxyFinder",
    "slug": "oxyFinder",
    "privacy": "public",
    "description": "oxyFinder is an app, where people can find available oxygen cylinder dealers near them. Sellers can also sell with this app.",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/oxyFinderIcon.png",
    "splash": {
      "image": "./assets/oxyFinderSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "syedlukman.oxyFinder",
      "adaptiveIcon": {
        "foregroundImage": "./assets/oxyFinderIcon1.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}